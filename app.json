{
  "expo": {
    "name": "Expensee",
    "slug": "ExpenseeApp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    
    "ios": {
      "bundleIdentifier":"de.deine.domaine.de",
      "supportsTablet": true
    },
    "android": {
      "package":"com.Expensee.application",
      "versionCode":3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions":[
        "CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}


